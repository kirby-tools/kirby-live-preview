{
  "name": "kirby-live-preview",
  "type": "module",
  "version": "1.0.2",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "dev": "PLAYGROUND=true kirbyup serve src/panel/index.js",
    "build": "kirbyup src/panel/index.js",
    "build:assets": "rm -rf assets && vite build --config src/assets/iframe/vite.config.js",
    "build:playground": "PLAYGROUND=true pnpm run build && pnpm run build:assets",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "archive": "pnpm run build && pnpm run build:assets && scripts/archive.sh",
    "release": "bumpp --all"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.3",
    "autoprefixer": "^10.4.18",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "kirbyup": "^3.1.5",
    "kirbyuse": "^0.4.1",
    "ohash": "^1.1.3",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.4.1",
    "throttleit": "^2.0.0",
    "ufo": "^1.5.2",
    "vite": "^5.1.6"
  }
}
